{
  "name": "enigma-mixin",
  "version": "0.1.0",
  "description": "Set of Qlik Sense enigma.js mixin",
  "author": {
    "email": "stefan.stoichev@gmail.com",
    "name": "Stefan Stoichev @countnazgul",
    "url": "https://sstoichev.eu"
  },
  "license": "MIT",
  "main": "dist/enigma-mixin.min.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "jest",
    "build-test": "rollup --config",
    "build": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "publish_local": "npm publish --registry http://192.168.0.10:4873"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.md",
    "LICENSE"
  ],
  "keywords": [
    "enigma.js",
    "enigma",
    "mixin",
    "qlik",
    "qlik-sense",
    "qlik-sense-desktop"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.13.9",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "enigma.js": "^2.7.2",
    "extend": "^3.0.2",
    "jest": "^24.9.0",
    "jest-html-reporter": "^3.3.0",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-uglify": "^6.0.4",
    "serialize-javascript": "^5.0.1",
    "ws": "^6.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "reporters": [
      "default",
      "./node_modules/jest-html-reporter"
    ]
  },
  "jest-html-reporter": {
    "pageTitle": "enigma.js mixins test suite",
    "outputPath": "test-report.html",
    "includeFailureMsg": true,
    "executionMode": "reporter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/countnazgul/enigma-mixin"
  },
  "bugs": {
    "url": "https://github.com/countnazgul/enigma-mixin/issues"
  }
}
