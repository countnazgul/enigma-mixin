{
  "name": "enigma-mixin",
  "version": "1.1.0",
  "description": "Set of Qlik Sense enigma.js mixin",
  "author": {
    "email": "stefan.stoichev@gmail.com",
    "name": "Stefan Stoichev @countnazgul",
    "url": "https://sstoichev.eu"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=14"
  },
  "engineStrict": true,
  "scripts": {
    "test": "set TS_NODE_PROJECT=tsconfig.test.json&mocha --require ts-node/register test/**/*.spec.ts",
    "build-test": "rollup --config",
    "build-prod": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "build": "rollup -c",
    "docs": "npx typedoc src/index.doc.ts --name \"Enigma Mixins\" --excludePrivate --hideGenerator --darkHighlightTheme dark-plus"
  },
  "module": "dist/index.es.js",
  "files": [
    "dist/*",
    "*.json",
    "*.md",
    "LICENSE",
    "!images/*",
    "!docs/*",
    "!tsconfig.test.json",
    "!tsconfig.json"
  ],
  "keywords": [
    "enigma.js",
    "enigma",
    "mixin",
    "qlik",
    "qlik-sense",
    "qlik-sense-desktop"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@types/chai": "^4.3.3",
    "@types/enigma.js": "^2.2.2",
    "@types/mocha": "^9.1.1",
    "@types/qlik-engineapi": "^12.67.12",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "enigma.js": "^2.9.0",
    "mocha": "^10.0.0",
    "rollup": "^2.78.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4",
    "ws": "^8.8.1"
  }
}