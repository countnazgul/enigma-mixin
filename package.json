{
  "name": "enigma-mixin",
  "version": "1.6.0",
  "description": "Set of Qlik Sense enigma.js mixin",
  "author": {
    "email": "stefan.stoichev@gmail.com",
    "name": "Stefan Stoichev @countnazgul",
    "url": "https://sstoichev.eu"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "engineStrict": true,
  "scripts": {
    "test": "vitest run",
    "build-test": "rollup --config",
    "build-prod": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "build": "rollup -c",
    "docs": "npx typedoc src/index.doc.ts --name \"Enigma Mixins\" --excludePrivate --hideGenerator --darkHighlightTheme dark-plus"
  },
  "module": "dist/index.es.js",
  "files": [
    "dist/*",
    "*.json",
    "*.md",
    "LICENSE",
    "!images/*",
    "!docs/*",
    "!tsconfig.test.json",
    "!tsconfig.json",
    "!docs"
  ],
  "keywords": [
    "enigma.js",
    "enigma",
    "mixin",
    "qlik",
    "qlik-sense",
    "qlik-sense-desktop"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/enigma.js": "^2.2.5",
    "@types/qlik-engineapi": "^12.67.16",
    "dotenv": "^16.4.1",
    "enigma.js": "^2.12.0",
    "rollup": "^4.9.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typescript": "^4.9.4",
    "vitest": "^0.34.6",
    "ws": "^8.16.0"
  },
  "dependencies": {
    "banditypes": "^0.2.5",
    "node-event-emitter": "^0.0.1"
  }
}
