{
  "name": "enigma-mixin",
  "version": "1.4.0",
  "description": "Set of Qlik Sense enigma.js mixin",
  "author": {
    "email": "stefan.stoichev@gmail.com",
    "name": "Stefan Stoichev @countnazgul",
    "url": "https://sstoichev.eu"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "engineStrict": true,
  "scripts": {
    "test": "set TS_NODE_PROJECT=tsconfig.test.json&mocha --require ts-node/register test/**/*.spec.ts --loader=ts-node/esm --no-warnings=ExperimentalWarning --experimental-specifier-resolution=node --ignore test/playground.spec.ts",
    "build-test": "rollup --config",
    "build-prod": "rollup -c && cross-env NODE_ENV=production rollup -c",
    "build": "rollup -c",
    "docs": "npx typedoc src/index.doc.ts --name \"Enigma Mixins\" --excludePrivate --hideGenerator --darkHighlightTheme dark-plus"
  },
  "module": "dist/index.es.js",
  "files": [
    "dist/*",
    "*.json",
    "*.md",
    "LICENSE",
    "!images/*",
    "!docs/*",
    "!tsconfig.test.json",
    "!tsconfig.json",
    "!docs"
  ],
  "keywords": [
    "enigma.js",
    "enigma",
    "mixin",
    "qlik",
    "qlik-sense",
    "qlik-sense-desktop"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@types/chai": "^4.3.4",
    "@types/enigma.js": "^2.2.2",
    "@types/mocha": "^10.0.1",
    "@types/qlik-engineapi": "^12.67.12",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "enigma.js": "^2.10.0",
    "mocha": "^10.2.0",
    "rollup": "^3.9.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "ws": "^8.12.0"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "tslib": "^2.4.1"
  }
}